<BaseClasses:SensorBase x:Class="Ester.Model.PlanControls.PlanGraph"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			xmlns:BaseClasses="clr-namespace:Ester.Model.BaseClasses"
                        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
                        xmlns:chartPlotter="clr-namespace:Ester.Model.UserControls.ChartPlotter"
                        MouseEnter="GraphOnMouseEnter" MouseLeave="GraphOnMouseLeave"
			Loaded="OnLoad" 
			 >
		<chartPlotter:ChartPlotter LineGraphs="{Binding}" NewLegendVisible="True" LegendVisibility="Collapsed" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" SnapsToDevicePixels="False" x:Name="Charter">
			<chartPlotter:ChartPlotter.LegendPanelTemplate>
				<ItemsPanelTemplate>
					<StackPanel Orientation="Vertical" Width="auto" Height="auto" />
				</ItemsPanelTemplate>
			</chartPlotter:ChartPlotter.LegendPanelTemplate>
			<chartPlotter:ChartPlotter.MainHorizontalAxis>
				<d3:HorizontalDateTimeAxis Height="50" />
			</chartPlotter:ChartPlotter.MainHorizontalAxis>
			<chartPlotter:ChartPlotter.MainVerticalAxis>
				<d3:VerticalAxis Width="20" />
			</chartPlotter:ChartPlotter.MainVerticalAxis>
		</chartPlotter:ChartPlotter>
</BaseClasses:SensorBase>
