    <Style x:Key="T2Style" TargetType="{x:Type Label}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Label}">
                    <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                        <Border Width="60" Height="20" Background="#F2836B">
                            <ContentPresenter HorizontalAlignment="Center" RecognizesAccessKey="True"
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" Margin="4,0"/>
                        </Border>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="Auto"/>
        <Setter Property="MinWidth" Value="60"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Foreground" Value="#FFFFFF"/>
    </Style>
